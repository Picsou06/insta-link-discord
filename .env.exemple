# Configuration Instagram
INSTAGRAM_USERNAME=votre_nom_utilisateur_instagram
INSTAGRAM_PASSWORD=votre_mot_de_passe_instagram

# Configuration Discord Webhook
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/YOUR_WEBHOOK_ID/YOUR_WEBHOOK_TOKEN

# Configuration du polling (optionnel)
# Nombre maximum d'erreurs avant pause
MAX_POLLING_ERRORS=10

# Délai de pause après trop d'erreurs (en millisecondes)
POLLING_DELAY_ON_ERROR=30000

# Configuration des heures de polling
# Heures de jour (format 24h)
DAY_START_HOUR=7
DAY_END_HOUR=19

# Délais de polling de jour (en millisecondes)
DAY_MIN_DELAY=5000
DAY_MAX_DELAY=20000

# Délais de polling de nuit (en millisecondes)
NIGHT_MIN_DELAY=30000
NIGHT_MAX_DELAY=60000
